    game.Players:Chat("/e a")
   game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("[Phone]").Parent = game:GetService("Players").LocalPlayer.Character
          game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Humanoid"):UnequipTools()
          
           if game.Players.LocalPlayer.Backpack:FindFirstChild("Punch") then 
          local punch = game.Players.LocalPlayer.Backpack:FindFirstChild("Punch")
  game:GetService("ReplicatedStorage").MainEvent:FireServer("RingTone",548348185)
          punch.Parent = game.Players.LocalPlayer.Character
