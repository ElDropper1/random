ToolsPage.AddToggle("Swing", nil, function(State)

    local Swing1 = Instance.new('Animation', game.ReplicatedStorage)
    Swing1.AnimationId = 'rbxassetid://8254794168'
    
    local Swing2 = Instance.new('Animation', game.ReplicatedStorage)
    Swing2.AnimationId = 'rbxassetid://8254792138'
    
    local Swing3 = Instance.new('Animation', game.ReplicatedStorage)
    Swing3.AnimationId = 'rbxassetid://8254789608'
    
    local Swing4 = Instance.new('Animation', game.ReplicatedStorage)
    Swing4.AnimationId = 'rbxassetid://8254787838'
    
    local _1 = game.Players.LocalPlayer.Character:FindFirstChildOfClass('Humanoid'):LoadAnimation(Swing1)
    local _2 = game.Players.LocalPlayer.Character:FindFirstChildOfClass('Humanoid'):LoadAnimation(Swing2)
    local _3 = game.Players.LocalPlayer.Character:FindFirstChildOfClass('Humanoid'):LoadAnimation(Swing3)
    local _4 = game.Players.LocalPlayer.Character:FindFirstChildOfClass('Humanoid'):LoadAnimation(Swing4)
    


    getgenv().swing = State
    while getgenv().swing do task.wait() 
    pcall(function()
    repeat task.wait()
    until game.Players.LocalPlayer.Character:FindFirstChild("FULLY_LOADED_CHAR")
            
    if not game.Players.LocalPlayer.Character:FindFirstChild("Swing") and not game.Players.LocalPlayer.Backpack:FindFirstChild("Swing") then
    
    local swingtool = Instance.new("Tool")
    swingtool.RequiresHandle = false 
    swingtool.Name = "Swing"
    swingtool.Parent = game.Players.LocalPlayer.Backpack
    swingtool.CanBeDropped = false
    
    swingtool.Activated:Connect(function()
        local Math = math.random(1, 4)
        if Math == 1 then
            _2:Stop()
            _3:Stop()
            _1:Play()
            _4:Stop()
            _1:AdjustSpeed(1.4)
            game:GetService("ReplicatedStorage").MainEvent:FireServer("RingTone",9109910173)
     game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("[Phone]").Parent = game:GetService("Players").LocalPlayer.Character
            game:GetService("Players").LocalPlayer.Character["[Phone]"].Parent = game:GetService("Players").LocalPlayer.Backpack
    game.Players:Chat("/e cm")
    task.wait()
    getgenv().Push = true
    task.wait(1.5)
    getgenv().Push = false
        elseif Math == 2 then
            _2:Play()
            _3:Stop()
            _1:Stop()
            _4:Stop()
            _2:AdjustSpeed(1.4)
            game:GetService("ReplicatedStorage").MainEvent:FireServer("RingTone",9109910173)
     game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("[Phone]").Parent = game:GetService("Players").LocalPlayer.Character
            game:GetService("Players").LocalPlayer.Character["[Phone]"].Parent = game:GetService("Players").LocalPlayer.Backpack
    game.Players:Chat("/e cm")
    task.wait()
    getgenv().Push = true
    task.wait(1.5)
    getgenv().Push = false
elseif Math == 3 then
    _2:Stop()
    _3:Play()
    _1:Stop()
    _4:Stop()
    _3:AdjustSpeed(1.4)
    game:GetService("ReplicatedStorage").MainEvent:FireServer("RingTone",9109910173)
    game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("[Phone]").Parent = game:GetService("Players").LocalPlayer.Character
           game:GetService("Players").LocalPlayer.Character["[Phone]"].Parent = game:GetService("Players").LocalPlayer.Backpack
   game.Players:Chat("/e cm")
task.wait()
getgenv().Push = true
task.wait(1.5)
getgenv().Push = false
elseif Math == 4 then
    _2:Stop()
    _3:Stop()
    _1:Stop()
    _4:Play()
    _4:AdjustSpeed(1.4)
    game:GetService("ReplicatedStorage").MainEvent:FireServer("RingTone",9109910173)
    game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("[Phone]").Parent = game:GetService("Players").LocalPlayer.Character
           game:GetService("Players").LocalPlayer.Character["[Phone]"].Parent = game:GetService("Players").LocalPlayer.Backpack
   game.Players:Chat("/e cm")
task.wait()
getgenv().Push = true
task.wait(1.5)
getgenv().Push = false
end
               end)
            end
        end)
      end
      if not getgenv().swing then 
        if  game.Players.LocalPlayer.Character:FindFirstChild("Swing") then 
            game.Players.LocalPlayer.Character:FindFirstChild("Swing"):Destroy()
        else
            game.Players.LocalPlayer.Backpack:FindFirstChild("Swing"):Destroy()
       end
       end
    end)


