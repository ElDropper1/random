          anim:Play()
          anim:AdjustSpeed(1.3)
          game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, -15)
         
          wait(1)
          game.Players:Chat("/e cm")
          task.wait()
  
          if game.Players.LocalPlayer.Character:FindFirstChild("Punch") then 
           b = true 
          end
          task.wait(2)
           game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("[Phone]").Parent = game:GetService("Players").LocalPlayer.Character
          game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Humanoid"):UnequipTools()
          task.wait()
          if b == true then 
          
          b = false 
            end 
          end
        end)
       end
    end)
   end
  if not getgenv().bad then 
      if  game.Players.LocalPlayer.Character:FindFirstChild("Punch") then 
          game.Players.LocalPlayer.Character:FindFirstChild("Punch"):Destroy()
      else
          game.Players.LocalPlayer.Backpack:FindFirstChild("Punch"):Destroy()
     end
    end
  end)

