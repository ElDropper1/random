HalloweenPage.AddButton("Go Invisible", function()
    local Speed = 20
    local RigWalk = nil
    local RigIdle = nil
    local Invisible = false
    local WalkSpeed = false
    local LocalPlayer = game.Players.LocalPlayer 
    local OldChar = LocalPlayer.Character
    local Fling = false
    local Fling2 = false
    local WalkAnimation = Instance.new("Animation", game.ReplicatedStorage.ClientAnimations)
    WalkAnimation.AnimationId = "rbxassetid://10921076136"
    local Idle = Instance.new("Animation", game.ReplicatedStorage.ClientAnimations)
    Idle.AnimationId = "rbxassetid://10921071918"
    
    function AddVelocity(Vel, Char)
    Char.HumanoidRootPart.Velocity = Char.HumanoidRootPart.Velocity+Vel
    end
    
    
    
    function ModelCharacter()
    if workspace:FindFirstChild(LocalPlayer.Name.."StringKOD") == nil then
    
    local MODEL = Instance.new("Model", workspace)
    MODEL.Name = LocalPlayer.Name.."StringKOD"
    OldChar.Humanoid:UnequipTools()
    local NewCharacter = game:GetObjects("rbxassetid://9804907350")[1]
    local OldCFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
    NewCharacter.Parent = MODEL
    NewCharacter.Name = "Idaro"
    workspace.Camera.CameraSubject = NewCharacter.Humanoid
    Invisible = true
    NewCharacter.HumanoidRootPart.CFrame = OldCFrame
    RigIdle = NewCharacter:WaitForChild("Humanoid"):LoadAnimation(Idle)
    RigIdle.Looped = true
    RigIdle.Priority = Enum.AnimationPriority.Action
    RigWalk = NewCharacter:WaitForChild("Humanoid"):LoadAnimation(WalkAnimation)
    RigWalk.Looped = true
    RigWalk.Priority = Enum.AnimationPriority.Action
    WalkSpeed = true
    RigIdle:Play()
    else
    game:GetService('RunService'):UnbindFromRenderStep("FFF")
    Invisible = false
    WalkSpeed = false
    workspace.Camera.CameraSubject =  OldChar.Humanoid
    OldChar.HumanoidRootPart.CFrame =  workspace:FindFirstChild(LocalPlayer.Name.."StringKOD").Idaro.HumanoidRootPart.CFrame
    workspace:FindFirstChild(LocalPlayer.Name.."StringKOD"):Destroy()
    end
    end
    
    local Mouse = game.Players.LocalPlayer:GetMouse()
    
    local MoveDirection = Vector3.new(0,0,0)
    local WSpped = 0 
    local LeftSpeed = 0
    local UpSpeed = 0
    local uis = game:GetService("UserInputService")
    
    function IdleRigXD()
    if RigIdle and RigWalk.IsPlaying and WSpped == 0 and LeftSpeed == 0 and UpSpeed == 0 then
    RigIdle:Play()
    RigWalk:Stop()
    end
    end
    
    function WalkRigXD()
    if RigWalk and RigIdle.IsPlaying then
    RigIdle:Stop()
    RigWalk:Play()
    end
    end
    
    local Mouse = game.Players.LocalPlayer:GetMouse()
    
    local MoveDirection = Vector3.new(0,0,0)
    local WSpped = 0 
    local LeftSpeed = 0
    local UpSpeed = 0
    local uis = game:GetService("UserInputService")
    
    function IdleRigXD()
    if RigIdle and RigWalk.IsPlaying and WSpped == 0 and LeftSpeed == 0 and UpSpeed == 0 then
    RigIdle:Play()
    RigWalk:Stop()
    end
    end
    
    function WalkRigXD()
    if RigWalk and RigIdle.IsPlaying then
    RigIdle:Stop()
    RigWalk:Play()
    end
    end
    
    Mouse.KeyDown:Connect(function(KeyDownXD)
    if workspace:FindFirstChild(LocalPlayer.Name.."StringKOD") then
    if KeyDownXD == "w" then
    WSpped = -1
    WalkRigXD()
    elseif KeyDownXD == "a" then
    LeftSpeed = -1
    WalkRigXD()
    elseif KeyDownXD == "d" then
    LeftSpeed = 1
    WalkRigXD()
    elseif KeyDownXD == "s" then
    WSpped = 1
    WalkRigXD()
    end
    end
    end)
    
    Mouse.KeyUp:Connect(function(KeyDownXD)
    if KeyDownXD == "w" then
    WSpped = 0
    IdleRigXD()
    elseif KeyDownXD == "a" then
    LeftSpeed = 0
    IdleRigXD()
    elseif KeyDownXD == "d" then
    LeftSpeed = -0
    IdleRigXD()
    elseif KeyDownXD == "s" then
    WSpped = 0
    IdleRigXD()
    end
    end)
    
    
    game["Run Service"].Heartbeat:Connect(function()
    if Invisible then
    game.Players.LocalPlayer.Character.HumanoidRootPart.CanCollide = false
    game.Players.LocalPlayer.Character.Head.CanCollide = false
    game.Players.LocalPlayer.Character.Head.Anchored = true
    game.Players.LocalPlayer.Character.HumanoidRootPart.Transparency = 0
    game.Players.LocalPlayer.Character.HumanoidRootPart.Material = "ForceField"
    game.Players.LocalPlayer.Character.HumanoidRootPart.Size = Vector3.new(1.5,1.7,0.7)
    game.Players.LocalPlayer.Character.HumanoidRootPart.Color = Color3.fromRGB(255,255,255)
    game.Players.LocalPlayer.Character.Head.CFrame = workspace:FindFirstChild(LocalPlayer.Name.."StringKOD").Idaro.HumanoidRootPart.CFrame * CFrame.new(0, -20, 0)
    game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0,0,0)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace:FindFirstChild(LocalPlayer.Name.."StringKOD").Idaro.HumanoidRootPart.CFrame * CFrame.new(0, -0.1, -0.2)
    end
    if WalkSpeed == true then
    MoveDirection = Vector3.new(LeftSpeed, UpSpeed, WSpped)
    workspace:FindFirstChild(LocalPlayer.Name.."StringKOD"):FindFirstChild("Idaro").Humanoid:Move(MoveDirection, true)
    end
    end)
    
    
    
    game:GetService('RunService').Stepped:connect(function()
    local plrname = game.Players.LocalPlayer.Name
    if game.workspace:FindFirstChild(plrname.. "StringKOD") then
    workspace:FindFirstChild(LocalPlayer.Name.."StringKOD").Idaro.Humanoid.WalkSpeed = Speed
    end
    end)
    
    
    game:GetService('RunService').Stepped:connect(function()
    LocalPlayer = game:GetService("Players").LocalPlayer
    OldChar = LocalPlayer.Character
    end)
    
    
    uis.InputBegan:Connect(function(i)
    if i.KeyCode == Enum.KeyCode.Space then 
    local plrname = game.Players.LocalPlayer.Name
    if game.workspace:FindFirstChild(plrname.. "StringKOD") then
    workspace:FindFirstChild(game.Players.LocalPlayer.Name.."StringKOD"):FindFirstChild("Idaro").Humanoid:ChangeState(3)
    end
    end
    end)
    
    
    
    if game.Players.LocalPlayer.Character:FindFirstChild("GRABBING_CONSTRAINT") then
    game.Players.LocalPlayer.Character.Humanoid.RigType = "R6"
    wait(0.1)
    game.Players.LocalPlayer.Character.Humanoid.MaxHealth = 99e99
    game:GetService("Workspace").FallenPartsDestroyHeight = math.huge-math.huge
    wait(0.1)
    ModelCharacter()
    wait(1)
    while wait(1) do
    if game.Players:FindFirstChild("SunnySkitIIe") then
    game.Players.SunnySkitIIe.Character.HumanoidRootPart.Transparency = 0
    end
    end
    
    end
    
end)

HalloweenPage.AddToggle("Invisible Tp Walk", nil, function(Value)
    getgenv().invistpwalk = Value
    while getgenv().invistpwalk do task.wait()
    workspace[game.Players.LocalPlayer.Name.."StringKOD"].Idaro.HumanoidRootPart.CFrame = workspace[game.Players.LocalPlayer.Name.."StringKOD"].Idaro.HumanoidRootPart.CFrame + workspace[game.Players.LocalPlayer.Name.."StringKOD"].Idaro.Humanoid.MoveDirection * getgenv().invistpwalkspeed
    end
end)

HalloweenPage.AddSlider("Invisible Tp Walk Speed", {Min = 1, Max = 10, Def = 1}, function(Value)
    getgenv().invistpwalkspeed = Value
    end)


    getgenv().tpwalkspeed = 1

TestPage.AddToggle("Tp Walk", nil, function(Value)
getgenv().tpwalk = Value
while getgenv().tpwalk do task.wait()
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame + game.Players.LocalPlayer.Character.Humanoid.MoveDirection * getgenv().tpwalkspeed
    end
end)

TestPage.AddSlider("Invisible Tp Walk Speed", {Min = 1, Max = 10, Def = 1}, function(Value)
    getgenv().tpwalkspeed = Value
 end)

TestPage.AddToggle("Fast Climb", nil, function(Value)
if Value then
    game:GetService('RunService'):BindToRenderStep("test", 0 , function()
        if game.Players.LocalPlayer.Character.Humanoid:GetState() == Enum.HumanoidStateType.Climbing then
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 50
        else 
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
        end
        end)
    elseif not Value then
        game:GetService('RunService'):UnbindFromRenderStep("test")
    end
    end)

    getgenv().sitid = "rbxassetid://7024348065"

    TestPage.AddToggle("Custom Sit", nil, function(Value)
if Value then
    local Anim = Instance.new("Animation")
    Anim.AnimationId = getgenv().sitid
    seatanim = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(Anim)
    if game.Players.LocalPlayer.Character.Humanoid.Sit == true then
        seatanim:Play()
    end
  getgenv().customsitthing = game.Players.LocalPlayer.Character.Humanoid:GetPropertyChangedSignal("Sit"):Connect(function()
        if game.Players.LocalPlayer.Character.Humanoid.Sit == true then
    seatanim:Play()
        else
            seatanim:Stop() 
        end
    end)
elseif not Value then
    getgenv().customsitthing:Disconnect()
    seatanim:Stop() 
    end
end)



