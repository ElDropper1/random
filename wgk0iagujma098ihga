kicktool.Parent = game.Players.LocalPlayer.Backpack
kicktool.CanBeDropped = false

kicktool.Activated:Connect(function()
 game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("[Phone]").Parent = game:GetService("Players").LocalPlayer.Character
        game:GetService("Players").LocalPlayer.Character["[Phone]"].Parent = game:GetService("Players").LocalPlayer.Backpack
        wait(0.000001)
         game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Kick").Parent = game:GetService("Players").LocalPlayer.Character

local kek = Instance.new("Animation")
kek.AnimationId = "rbxassetid://2788306916"
local humanoid = game.Players.LocalPlayer.Character:WaitForChild("Humanoid")
local anim = humanoid:LoadAnimation(kek)
anim:Play()
anim:AdjustSpeed(1)
wait(0.3)
game:GetService("ReplicatedStorage").MainEvent:FireServer("RingTone",8907573127)
game.Players:Chat("/e cm")
task.wait()
getgenv().Kicks = true
task.wait(2.4)
getgenv().Kicks = false
           end)
        end
    end)
  end
  if not getgenv().kick then 
    if  game.Players.LocalPlayer.Character:FindFirstChild("Kick") then 
        game.Players.LocalPlayer.Character:FindFirstChild("Kick"):Destroy()
    else
        game.Players.LocalPlayer.Backpack:FindFirstChild("Kick"):Destroy()
   end
   end
end)

