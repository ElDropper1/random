--// Anti Cheat Bypass

assert(getrawmetatable)
gmt = getrawmetatable(game)
setreadonly(gmt, false)
old = gmt.__namecall
gmt.__namecall =
    newcclosure(
        function(self, ...)
        local args = {...}
        if tostring(args[1]) == "BreathingHAMON" then
            return
        elseif tostring(args[1]) == "TeleportDetect" then
            return
        elseif tostring(args[1]) == "CHECKER_1" then
            return
        elseif tostring(args[1]) == "CHECKER" then
            return
        elseif tostring(args[1]) == "GUI_CHECK" then
            return
        elseif tostring(args[1]) == "OneMoreTime" then
            return
        elseif tostring(args[1]) == "checkingSPEED" then
            return
        elseif tostring(args[1]) == "BANREMOTE" then
            return
        elseif tostring(args[1]) == "PERMAIDBAN" then
            return
        elseif tostring(args[1]) == "KICKREMOTE" then
            return
        elseif tostring(args[1]) == "BR_KICKPC" then
            return
        end
        return old(self, ...)
        end)

game:GetService('StarterGui'):SetCore("SendNotification", {
	Title = '';
	Text = 'Anti cheat bypass executed';
	Icon = "";
	Duration = 30;
});

--// Hide/Unhide

game.Players.LocalPlayer.Chatted:Connect(function(Message)
    if Message == '/e hide' then task.wait()
        game:GetService("CoreGui")["Godmode Gui"].ContainerFrame.Visible = false

elseif Message == '/e unhide' then task.wait()
    game:GetService("CoreGui")["Godmode Gui"].ContainerFrame.Visible = true
 end
end)


