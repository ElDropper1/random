    getgenv().knifesfx = Value
    while getgenv().knifesfx do task.wait()
    
        for i,v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do 
            if v:IsA("Sound") and v.Name == "Stab"  then 
            if not knifesfxplayed then
    game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("[Phone]").Parent = game:GetService("Players").LocalPlayer.Character
        game:GetService("Players").LocalPlayer.Character["[Phone]"].Parent = game:GetService("Players").LocalPlayer.Backpack
    game:GetService("ReplicatedStorage").MainEvent:FireServer("RingTone",7350477734)
    game.Players:Chat("/e cm")
    knifesfxplayed = true
    wait(1)
    knifesfxplayed = false
              end 
           end
        end 
    end
end)



MainPage.AddButton("Tp To Bull", function()
 game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-665.001526, 24.3948212, -106.292496, -0.000192354855, 0.10302107, 0.994679272, 0.000973766844, 0.994678795, -0.103020892, -0.999999523, 0.000948773697, -0.000291650009)
