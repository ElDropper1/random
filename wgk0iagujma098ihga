MainPage.AddToggle("Spider Sense (meele)", nil, function(Value)
    getgenv().spidersense = Value

    local tools = {
        "Combat",
        "[Nunchucks]",
        "[Knife]",
        "[Bat]",
        "[StopSign]",
        "[SledgeHammer]",
        "[Pencil]",
        "[Katana]"
    }
    
    local function findTool(toolName)
        for i, v in ipairs(tools) do
            if v == toolName then
                return true
            end
        end
        return false
    end
    
    local function isWithinDistance(player, distance)
        if game:GetService("Players").LocalPlayer.Character and game:GetService("Players").LocalPlayer.Character:FindFirstChild("HumanoidRootPart") and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
            local localPosition = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position
            local playerPosition = player.Character.HumanoidRootPart.Position
            local distanceBetween = (localPosition - playerPosition).Magnitude
            return distanceBetween <= distance
        end
        return false
    end
    
    local autism = false
    spawn(function()
        while getgenv().spidersense do 
            task.wait()
            pcall(function()
                for i, player in ipairs(game:GetService("Players"):GetChildren()) do 
                    if player ~= game:GetService("Players").LocalPlayer and isWithinDistance(player, 5) then
                        local character = player.Character
                        if character and character:FindFirstChild("Humanoid") then
                            local tool = character:FindFirstChildOfClass("Tool")
                            if tool and findTool(tool.Name) and player.Character.BodyEffects.Attacking.Value == true  then 
                                local AnimationTracks = game.Players.LocalPlayer.Character.Humanoid:GetPlayingAnimationTracks()
                                for i, Track in pairs(AnimationTracks) do
                                  if Track.Animation.AnimationId == "rbxassetid://2788315673" or Track.Animation.AnimationId == "rbxassetid://2788316350" or Track.Animation.AnimationId == "rbxassetid://2788313790"  then
                                  autism = true 
                                  elseif Track.Animation.AnimationId ~= "rbxassetid://2788315673" or Track.Animation.AnimationId ~= "rbxassetid://2788316350" or Track.Animation.AnimationId ~= "rbxassetid://2788313790" then 
                                    if autism == false then 
                                    autism = true
                                    local a = math.random(1,4)
                                    if a == 1 then 
                                        game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("[Phone]").Parent = game:GetService("Players").LocalPlayer.Character
                                        game:GetService("Players").LocalPlayer.Character["[Phone]"].Parent = game:GetService("Players").LocalPlayer.Backpack
                                game:GetService("ReplicatedStorage").MainEvent:FireServer("RingTone",3950015905)
                                game.Players:Chat("/e cm")
                                    local Anim = Instance.new("Animation")
                                    Anim.AnimationId = "http://www.roblox.com/asset/?id=2788315673"
                                    CurrAnim = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(Anim)
                                    CurrAnim:Play()
                                    CurrAnim.Stopped:Wait()
                                    autism = false 
                                    end 
                                    if a == 2 then 
                                        game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("[Phone]").Parent = game:GetService("Players").LocalPlayer.Character
                                        game:GetService("Players").LocalPlayer.Character["[Phone]"].Parent = game:GetService("Players").LocalPlayer.Backpack
                                game:GetService("ReplicatedStorage").MainEvent:FireServer("RingTone",3950015905)
                                game.Players:Chat("/e cm")
                                        local Anim = Instance.new("Animation")
                                        Anim.AnimationId = "http://www.roblox.com/asset/?id=2788316350"
                                        CurrAnim = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(Anim)
                                        CurrAnim:Play()
                                        CurrAnim.Stopped:Wait()
                                        autism = false 
                                    end
                                    if a == 3 then 
                                        game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("[Phone]").Parent = game:GetService("Players").LocalPlayer.Character
                                        game:GetService("Players").LocalPlayer.Character["[Phone]"].Parent = game:GetService("Players").LocalPlayer.Backpack
                                game:GetService("ReplicatedStorage").MainEvent:FireServer("RingTone",3950015905)
                                game.Players:Chat("/e cm")
                                        local Anim = Instance.new("Animation")
                                        Anim.AnimationId = "http://www.roblox.com/asset/?id=2788313790"
                                        CurrAnim = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(Anim)
                                        CurrAnim:Play()
                                        CurrAnim.Stopped:Wait()
                                        autism = false 
                                    end
                                    if a == 4 then 
                                        game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("[Phone]").Parent = game:GetService("Players").LocalPlayer.Character
                                        game:GetService("Players").LocalPlayer.Character["[Phone]"].Parent = game:GetService("Players").LocalPlayer.Backpack
                                game:GetService("ReplicatedStorage").MainEvent:FireServer("RingTone",3950015905)
                                game.Players:Chat("/e cm")
                                        local Anim = Instance.new("Animation")
                                        Anim.AnimationId = "http://www.roblox.com/asset/?id=2788314837"
                                        CurrAnim = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(Anim)
                                        CurrAnim:Play()
                                        CurrAnim.Stopped:Wait()
                                        autism = false 
                                    end
                                    end
                                  end
                                end
                            end
                        end
                    end
                end
            end)
        end
    end)
end)





MainPage.AddToggle("Hit Sfx", nil, function(Value)
    local hitsfxplayed = false

    getgenv().hitsfx2 = Value
    while getgenv().hitsfx2 do task.wait()
    
        for i,v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do 
            if v:IsA("Sound") and v.Name == "Punch1" or v.Name == "Punch2" or v.Name == "Punch3" or v.Name == "Punch4" then 
            if not hitsfxplayed then
    game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("[Phone]").Parent = game:GetService("Players").LocalPlayer.Character
        game:GetService("Players").LocalPlayer.Character["[Phone]"].Parent = game:GetService("Players").LocalPlayer.Backpack
    game:GetService("ReplicatedStorage").MainEvent:FireServer("RingTone",7102498124)
    game.Players:Chat("/e cm")
   hitsfxplayed = true
    wait(1)
    hitsfxplayed = false
              end 
           end
        end 
    end
end)

