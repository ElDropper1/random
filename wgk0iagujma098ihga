            if descendant.Name == 'Equipped' then 
            for i, v in pairs(descendant:GetChildren()) do 
                if v.Name == 'Edge' then 
                   v.BackgroundTransparency = 1
                end
            end
            local UIStroke = Instance.new('UIStroke', descendant.Parent)
            UIStroke.Color = Color3.fromRGB(255,255,255)
            UIStroke.Thickness = 2
            UIStroke.ApplyStrokeMode = 'Border'
        
            end
            end)
        
            getgenv().unequipped = game:GetService("CoreGui").RobloxGui.Backpack.Hotbar.DescendantRemoving:connect(function(descendant)
                if descendant.Name == 'Equipped' then 
                    if descendant.Parent:FindFirstChildWhichIsA("UIStroke") then 
                        descendant.Parent:FindFirstChildWhichIsA("UIStroke"):Destroy()
                    end
                end
            end)
        end
