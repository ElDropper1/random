MainPage.AddButton("Mask", function()
    local player = game.Players.LocalPlayer
    local firstPos = player.Character.HumanoidRootPart.Position
    
    local function clickDetectorExists()
        return game:GetService("Workspace").Ignored.Shop:FindFirstChild("[Surgeon Mask] - $26") and
               game:GetService("Workspace").Ignored.Shop["[Surgeon Mask] - $26"]:FindFirstChild("ClickDetector")
    end
    
    local function maskInBackpack()
        return player.Backpack:FindFirstChild("[Mask]")
    end
    
    local function maskEquipped()
        return player.Character:FindFirstChild("In-gameMask")
    end
    
    local function removeTools()
        game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):UnequipTools()
    end
    
    local function waitFor(condition)
        repeat
            wait()
        until condition()
    end
    
    waitFor(clickDetectorExists)
    repeat
    player.Character.HumanoidRootPart.CFrame = CFrame.new(game:GetService("Workspace").Ignored.Shop["[Surgeon Mask] - $26"].Head.Position) + Vector3.new(0,-8,0)
    fireclickdetector(game:GetService("Workspace").Ignored.Shop["[Surgeon Mask] - $26"].ClickDetector)
    task.wait()
    until  player.Backpack:FindFirstChild("[Mask]")

    waitFor(maskInBackpack)
    
    player.Backpack:FindFirstChild("[Mask]").Parent = player.Character
    player.Character:FindFirstChild("[Mask]"):Activate()
    
    waitFor(maskEquipped)
    
    waitFor(function()
        removeTools()
        return maskInBackpack()
    end)
    
    task.wait(0.5)
    
    player.Character.HumanoidRootPart.CFrame = CFrame.new(firstPos)
end)

MainPage.AddButton("Remove Arm", function()
    local target = "ElDropper22"
game.Players[target].Character.RightLowerArm.Position = Vector3.new(0,-1000,0)
end)

MainPage.AddButton("Fall", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-264.0344, 230, -358.0697, 0.035, 0, 0.9994, 0, 1, 0, -0.9994, 0, 0.035)
    local FallAnim = Instance.new("Animation")
    FallAnim.AnimationId = "rbxassetid://13850654420"
    FallAnim = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(FallAnim)
    FallAnim:Play()
    game:GetService('RunService'):BindToRenderStep("Land", 0 , function()
    if game.Players.LocalPlayer.Character.Humanoid:GetState() == Enum.HumanoidStateType.Landed then
    FallAnim:Stop()
    local LandAnim = Instance.new("Animation")
    LandAnim.AnimationId = "rbxassetid://13850663836"
    LandAnim = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(LandAnim)
    LandAnim:Play()
     game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("[Phone]").Parent = game:GetService("Players").LocalPlayer.Character
    game:GetService("Players").LocalPlayer.Character["[Phone]"].Parent = game:GetService("Players").LocalPlayer.Backpack
    game:GetService("ReplicatedStorage").MainEvent:FireServer("RingTone",13850747895)
    game.Players:Chat("/e cm")
             game:GetService('RunService'):UnbindFromRenderStep("Land")
          end
    end)
end)

ToolsPage.AddToggle("Shazam Fly", false, function(State)
    getgenv().fly = State
    while getgenv().fly == true do task.wait() 
        pcall(function()
        if not game.Players.LocalPlayer.Character:FindFirstChild("Fly") and not game.Players.LocalPlayer.Backpack:FindFirstChild("Fly") then
            
       function sandbox(var,func)
                local env = getfenv(func)
                local newenv = setmetatable({},{
                    __index = function(self,k)
                        if k=="script" then
                            return var
                        else
                            return env[k]
                        end
                    end,
                })
                setfenv(func,newenv)
                return func
            end
            cors = {}
            mas = Instance.new("Model",game:GetService("Lighting"))
            Tool0 = Instance.new("Tool")
            LocalScript1 = Instance.new("LocalScript")
            Sound2 = Instance.new("Sound")
            Sound3 = Instance.new("Sound")
            Animation4 = Instance.new("Animation")
            RemoteEvent5 = Instance.new("RemoteEvent")
            Animation6 = Instance.new("Animation")
            Tool0.Name = "Fly"
            Tool0.Parent = mas
            Tool0.CanBeDropped = false
            Tool0.RequiresHandle = false
            LocalScript1.Name = "local"
            LocalScript1.Parent = Tool0
            table.insert(cors,sandbox(LocalScript1,function()
                wait();
                local l__LocalPlayer__1 = game.Players.LocalPlayer;
                while true do
                    wait();
                    if l__LocalPlayer__1.Character then
                        break;
                    end;
                end;
                local l__Character__2 = l__LocalPlayer__1.Character;
                local l__Humanoid__3 = l__Character__2:WaitForChild("Humanoid");
                local l__Parent__4 = script.Parent;
                local u1 = false;
                local u2 = l__Humanoid__3:LoadAnimation(script.Hover);
                local u3 = nil;
                local u4 = nil;
                local u5 = false;
                local u6 = false;
                local u7 = l__Humanoid__3:LoadAnimation(script.Fly);
                l__Parent__4.Equipped:connect(function(p1)
                    Sound3:Play()
                    p1.TargetFilter = workspace;
                    u1 = true;
                    u2:Play();
                    u3 = Instance.new("BodyGyro");
                    u3.Name = "NA";
                    u3.Parent = l__Character__2.HumanoidRootPart;
                    u3.MaxTorque = Vector3.new(30000, 0, 30000);
                    u3.P = u3.P * 5;
                    u3.CFrame = CFrame.new(0, 0, 0);
                    u4 = Instance.new("BodyPosition");
                    u4.Name = "NA";
                    u4.Parent = l__Character__2.HumanoidRootPart;
                    u4.MaxForce = Vector3.new(60000, 60000, 60000);
                     u4.Position = l__Character__2.HumanoidRootPart.Position
                    p1.Button1Down:connect(function()
                        Sound2:Play()
                        if u5 then
                            return;
                        end;
                        u5 = true;
                        u6 = true;
                        u7:Play();
                        local v5 = Instance.new("BodyVelocity", l__Character__2.HumanoidRootPart);
                        v5.MaxForce = Vector3.new(50000, 50000, 50000);
                        v5.Velocity = CFrame.new(l__Character__2.HumanoidRootPart.CFrame.p, p1.Hit.p).lookVector * 85;
                        u3.MaxTorque = Vector3.new(30000, 30000, 30000);
                        u3.CFrame = CFrame.new(l__Character__2.HumanoidRootPart.CFrame.p, p1.Hit.p);
                        u4.Parent = nil;
                        l__Humanoid__3.AutoRotate = false;
                        if u2.IsPlaying then
                            u2:Stop();
                        end;
                        while u6 do
                            v5.Velocity = CFrame.new(l__Character__2.HumanoidRootPart.CFrame.p, p1.Hit.p).lookVector * 85;
                            u3.CFrame = CFrame.new(l__Character__2.HumanoidRootPart.CFrame.p, p1.Hit.p);
                            game:GetService("RunService").Heartbeat:wait();		
                        end;
                        l__Humanoid__3.AutoRotate = true;
                        v5:Destroy();
                        if u4 ~= nil then
                            u4.Position = l__Character__2.HumanoidRootPart.Position;
                            u4.Parent = l__Character__2.HumanoidRootPart;
                        end;
                        if u3 ~= nil then
                            u3.MaxTorque = Vector3.new(30000, 0, 30000);
                            u3.CFrame = CFrame.new(0, 0, 0);
                        end;
                        u6 = false;
                        if u1 then
                            u2:Play();
                        end;
                        u7:Stop();
                        wait(0.3);
                        u5 = false;
                    end);
                    p1.Button1Up:connect(function()
                        Sound2:Stop()
                        Sound3:Play()
                        u6 = false;
                    end);
                end);
                l__Parent__4.Unequipped:connect(function()
                    Sound2:Stop()
                    Sound3:Play()
                    u1 = false;
                    u6 = false;
                    u3:Destroy();
                    u3 = nil;
                    u4:Destroy();
                    u4 = nil;
                    if u2.IsPlaying then
                        u2:Stop();
                    end;
                end);
            end))
            Sound2.Name = "Flapping"
            Sound2.Parent = LocalScript1
            Sound2.Looped = true
            Sound2.MaxDistance = 100
            Sound2.Pitch = 1.5
            Sound2.PlaybackSpeed = 1.5
            Sound2.SoundId = "rbxassetid://1462718291"
            Sound3.Name = "Activation"
            Sound3.Parent = LocalScript1
            Sound3.MaxDistance = 100
            Sound3.SoundId = "rbxassetid://2952274135"
            Sound3.Volume = 0.69999998807907
            Animation4.Name = "Hover"
            Animation4.Parent = LocalScript1
            Animation4.AnimationId = "rbxassetid://3541114300"
            RemoteEvent5.Name = "re"
            RemoteEvent5.Parent = LocalScript1
            Animation6.Name = "Fly"
            Animation6.Parent = LocalScript1
            Animation6.AnimationId = "rbxassetid://3541044388"
            for i,v in pairs(mas:GetChildren()) do
                v.Parent = game.Players.LocalPlayer.Backpack
                pcall(function() v:MakeJoints() end)
            end
            mas:Destroy()
            for i,v in pairs(cors) do
                spawn(function()
                    pcall(v)
                end)
            end
      
    
        end
      end)
    end
    if not getgenv().fly then 
        if  game.Players.LocalPlayer.Character:FindFirstChild("Fly") then 
            game.Players.LocalPlayer.Character:FindFirstChild("Fly"):Destroy()
        else
            game.Players.LocalPlayer.Backpack:FindFirstChild("Fly"):Destroy()
           end
        end
    end)

    ToolsPage.AddToggle("Ghost Fly", false, function(State)
      getgenv().ghostfly = State
        while getgenv().ghostfly == true do task.wait() 
            pcall(function()
            if not game.Players.LocalPlayer.Character:FindFirstChild("Ghost Fly") and not game.Players.LocalPlayer.Backpack:FindFirstChild("Ghost Fly") then
                
                local function Fly()
                    local function sandbox(var, func)
                        local env = getfenv(func)
                        local newenv = setmetatable({}, {
                            __index = function(self, k)
                                if k == "script" then
                                    return var
                                else
                                    return env[k]
                                end
                            end,
                        })
                        setfenv(func, newenv)
                        return func
                    end
            
                    local coroutines = {}
                    local model = Instance.new("Model", game:GetService("Lighting"))
                    local tool = Instance.new("Tool")
                    local localScript = Instance.new("LocalScript")
                    local sound1 = Instance.new("Sound")
                    local sound2 = Instance.new("Sound")
                    local animation1 = Instance.new("Animation")
                    local remoteEvent = Instance.new("RemoteEvent")
                    local animation2 = Instance.new("Animation")
            
                    tool.Name = "Ghost Fly"
                    tool.Parent = model
                    tool.CanBeDropped = false
                    tool.RequiresHandle = false
                    localScript.Name = "local"
                    localScript.Parent = tool
            
                    table.insert(coroutines, sandbox(localScript, function()
                        wait()
                        local localPlayer = game.Players.LocalPlayer
                        while true do
                            wait()
                            if localPlayer.Character then
                                break
                            end
                        end
                        local character = localPlayer.Character
                        local humanoid = character:WaitForChild("Humanoid")
                        local parent = script.Parent
                        local isButton1Down = false
                        local hoverAnimation = humanoid:LoadAnimation(script.Hover)
                        local bodyGyro = nil
                        local bodyPosition = nil
                        local isButton1Up = false
                        local isFlying = false
                        local flyAnimation = humanoid:LoadAnimation(script.Fly)
            
                        parent.Equipped:Connect(function(inputObject)
                         
                            game:GetService('RunService'):BindToRenderStep("Noclip", 0 , function()
                            if game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then 
                                game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart").CanCollide = false
                            end
                            if game.Players.LocalPlayer.Character:FindFirstChild("UpperTorso") then 
                                game.Players.LocalPlayer.Character:FindFirstChild("UpperTorso").CanCollide = false
                            end
                            if game.Players.LocalPlayer.Character:FindFirstChild("Head") then 
                                game.Players.LocalPlayer.Character:FindFirstChild("Head").CanCollide = false
                            end
                            if game.Players.LocalPlayer.Character:FindFirstChild("LowerTorso") then 
                                game.Players.LocalPlayer.Character:FindFirstChild("LowerTorso").CanCollide = false
                            end
                        end)

                            sound2:Play()
                            inputObject.TargetFilter = workspace
                            isButton1Down = true
                            hoverAnimation:Play()

                            bodyGyro = Instance.new("BodyGyro")
                            bodyGyro.Name = "NA"
                            bodyGyro.Parent = character.HumanoidRootPart
                            bodyGyro.MaxTorque = Vector3.new(30000, 0, 30000)
                            bodyGyro.P = bodyGyro.P * 5
                            bodyGyro.CFrame = CFrame.new(0, 0, 0)
                            bodyPosition = Instance.new("BodyPosition")
                            bodyPosition.Name = "NA"
                            bodyPosition.Parent = character.HumanoidRootPart
                            bodyPosition.MaxForce = Vector3.new(60000, 60000, 60000)
                            bodyPosition.Position = character.HumanoidRootPart.Position + Vector3.new(0,8,0)
            
                            inputObject.Button1Down:Connect(function()
                                sound1:Play()
                                if isFlying then
                                    return
                                end
                                isFlying = true
                                isButton1Up = true
                                flyAnimation:Play()
                                hoverAnimation:Play()
                                local bodyVelocity = Instance.new("BodyVelocity", character.HumanoidRootPart)
                                bodyVelocity.MaxForce = Vector3.new(50000, 50000, 50000)
                                bodyVelocity.Velocity = CFrame.new(character.HumanoidRootPart.CFrame.p, inputObject.Hit.p).lookVector * 25
                                bodyGyro.MaxTorque = Vector3.new(30000, 30000, 30000)
                                bodyGyro.CFrame = CFrame.new(character.HumanoidRootPart.CFrame.p, inputObject.Hit.p)
                                bodyPosition.Parent = nil
                                humanoid.AutoRotate = false
             

                                while isButton1Up do
                                    bodyVelocity.Velocity = CFrame.new(character.HumanoidRootPart.CFrame.p, inputObject.Hit.p).lookVector * 25
                                    bodyGyro.CFrame = CFrame.new(character.HumanoidRootPart.CFrame.p, inputObject.Hit.p)
                                    game:GetService("RunService").Heartbeat:Wait()
                                end
                                humanoid.AutoRotate = true
                                bodyVelocity:Destroy()
                                if bodyPosition ~= nil then
                                    bodyPosition.Position = character.HumanoidRootPart.Position
                                    bodyPosition.Parent = character.HumanoidRootPart
                                end
                                if bodyGyro ~= nil then
                                    bodyGyro.MaxTorque = Vector3.new(30000, 0, 30000)
                                    bodyGyro.CFrame = CFrame.new(0, 0, 0)
                                end
                                isButton1Up = false
                                if isButton1Down then
                                    hoverAnimation:Play()
                                    flyAnimation:Play()
                                end
                                flyAnimation:Stop()
                                wait(0.3)
                                isFlying = false
                            end)
            
                            inputObject.Button1Up:Connect(function()
                                sound1:Stop()
                                sound2:Play()
                                isButton1Up = false
                            end)
                        end)
            
                        parent.Unequipped:Connect(function()
                            game:GetService('RunService'):UnbindFromRenderStep("Noclip")
                            task.wait()
                            if game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then 
                                game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart").CanCollide = true
                            end
                            if game.Players.LocalPlayer.Character:FindFirstChild("UpperTorso") then 
                                game.Players.LocalPlayer.Character:FindFirstChild("UpperTorso").CanCollide = true
                            end
                            if game.Players.LocalPlayer.Character:FindFirstChild("LowerTorso") then 
                                game.Players.LocalPlayer.Character:FindFirstChild("LowerTorso").CanCollide = true
                            end
                            sound1:Stop()
                            sound2:Play()
                            isButton1Down = false
                            isButton1Up = false
                            bodyGyro:Destroy()
                            bodyGyro = nil
                            bodyPosition:Destroy()
                            bodyPosition = nil
                            if hoverAnimation.IsPlaying then
                                hoverAnimation:Stop()
                            end
                        end)
                    end))
            
                    sound1.Name = "Flapping"
                    sound1.Parent = localScript
                    sound1.Looped = true
                    sound1.MaxDistance = 100
                    sound1.Pitch = 1.5
                    sound1.PlaybackSpeed = 1.5
                    sound1.SoundId = "rbxassetid://1462718291"
                    sound2.Volume = 0
            
                    sound2.Name = "Activation"
                    sound2.Parent = localScript
                    sound2.MaxDistance = 100
                    sound2.SoundId = "rbxassetid://2952274135"
                    sound2.Volume = 0
            
                    animation1.Name = "Hover"
                    animation1.Parent = localScript
                    animation1.AnimationId = "rbxassetid://3541114300"
            
                    remoteEvent.Name = "re"
                    remoteEvent.Parent = localScript
            
                    animation2.Name = "Fly"
                    animation2.Parent = localScript
                    animation2.AnimationId = "rbxassetid://3084858603"
            
                    for _, v in pairs(model:GetChildren()) do
                        v.Parent = game.Players.LocalPlayer.Backpack
                        pcall(function()
                            v:MakeJoints()
                        end)
                    end
            
                    model:Destroy()
            
                    for _, v in pairs(coroutines) do
                        spawn(function()
                            pcall(v)
                        end)
                    end
                end
            
                game.Players.LocalPlayer.Character:WaitForChild("FULLY_LOADED_CHAR")
                Fly()
            
            end
          end)
        end
        if not getgenv().ghostfly then 
            game:GetService('RunService'):UnbindFromRenderStep("Speed")
            if  game.Players.LocalPlayer.Character:FindFirstChild("Ghost Fly") then 
                game.Players.LocalPlayer.Character:FindFirstChild("Ghost Fly"):Destroy()
            else
                game.Players.LocalPlayer.Backpack:FindFirstChild("Ghost Fly"):Destroy()
               end
            end
        end)


