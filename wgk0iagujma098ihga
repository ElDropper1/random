               sprintool.Name = "Grapple"
               sprintool.Parent = game.Players.LocalPlayer.Backpack
               sprintool.CanBeDropped = false
               local swinga = false
               
               sprintool.Activated:Connect(function()
               if  swinga == false then 
                swinga = true 
               elseif swinga == true then 
                swinga = false
               end
               if swinga then
    

                local starss = Instance.new("Animation")
                starss.AnimationId = "rbxassetid://3090585357"
                sanims = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(starss)
                sanims:Play()
    
                wait(0.15)
    
                game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("[Phone]").Parent = game:GetService("Players").LocalPlayer.Character
                game:GetService("Players").LocalPlayer.Character["[Phone]"].Parent = game:GetService("Players").LocalPlayer.Backpack
                game:GetService("Players").LocalPlayer.Character["Grapple"].Parent = game:GetService("Players").LocalPlayer.Backpack
                game:GetService("ReplicatedStorage").MainEvent:FireServer("RingTone",4780469887)
                game.Players:Chat("/e cm")
                task.wait(0.001)
                game:GetService("Players").LocalPlayer.Backpack["Grapple"].Parent = game:GetService("Players").LocalPlayer.Character

    local localPlayer = game.Players.LocalPlayer
     
    
    local beam = Instance.new("Beam")
    beam.Segments = 1
    beam.Width0 = 0.1
    beam.Width1 = 0.1
    beam.Color = ColorSequence.new(Color3.new(0, 0, 0))
    beam.FaceCamera = true
     
    
    local attachment0 = Instance.new("Attachment")
    local attachment1 = Instance.new("Attachment")
    beam.Attachment0 = attachment0
    beam.Attachment1 = attachment1
     
    
    beam.Parent = workspace.Terrain
    attachment0.Parent = workspace.Terrain
    attachment1.Parent = workspace.Terrain
     
    
    local mouse = game.Players.LocalPlayer:GetMouse()
     
    
     
    
     local character = localPlayer.Character
     if not character then
    
      beam.Enabled = false
      return
     end
     
    
     local head = character:FindFirstChild("RightHand")
     if not head then
    
      beam.Enabled = false
      return
     end
     
    
     beam.Enabled = true
     
    
     local origin = head.Position
     local finish = mouse.Hit.p
     
    
     attachment0.Position = origin
     attachment1.Position = finish
    
    


    
                local grappling = Instance.new("Animation")
                grappling.AnimationId = "rbxassetid://3090583403"
                ganim = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(grappling)
                ganim:Play()
          task.wait()
    
    
       
                
          local Rotation = game:GetService('Players').LocalPlayer.Character:FindFirstChild('HumanoidRootPart').CFrame - game:GetService('Players').LocalPlayer.Character:FindFirstChild('HumanoidRootPart').Position;
          local Tween = game:GetService('TweenService'):Create(game:GetService('Players').LocalPlayer.Character:FindFirstChild('HumanoidRootPart'), TweenInfo.new(2, Enum.EasingStyle.Linear), {CFrame = CFrame.new(game:GetService('Players').LocalPlayer:GetMouse().Hit.X, game:GetService('Players').LocalPlayer:GetMouse().Hit.Y + 5, game:GetService('Players').LocalPlayer:GetMouse().Hit.Z) * Rotation})
          Tween:Play();
          game:GetService("TweenService"):Create(workspace.CurrentCamera, TweenInfo.new(0.3), {FieldOfView = oldfov}):Play()
    wait(2)
    ganim:Stop()
    beam.Enabled = false
        sanims:Stop()
        game:GetService("TweenService"):Create(workspace.CurrentCamera, TweenInfo.new(0.3), {FieldOfView = oldfov}):Play()
        local landa = Instance.new("Animation")
        landa.AnimationId = "rbxassetid://3090584355"
        lanim = game.Players.LocalPlayer.Character.Humanoid.Animator:LoadAnimation(landa)
        lanim:Play()
        wait(0.35)
        lanim:Stop()
                  game:GetService('RunService'):BindToRenderStep("qaf", 0 , function()
               end)
               end
               if swinga == false then 
                game:GetService('RunService'):UnbindFromRenderStep("qaf")
                task.wait()
                 
               end
               game:GetService("Players").LocalPlayer.CharacterAdded:Connect(function()
                  game:GetService('RunService'):UnbindFromRenderStep("qaf")
               end)  
            
               end)
              end
             end)
            end
            if not getgenv().bad then 
                if  game.Players.LocalPlayer.Character:FindFirstChild("Grapple") then 
                    game.Players.LocalPlayer.Character:FindFirstChild("Grapple"):Destroy()
                else
                    game.Players.LocalPlayer.Backpack:FindFirstChild("Grapple"):Destroy()
               end
              end
            end)


