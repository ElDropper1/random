              if not getgenv().uwufly then 
                  if  game.Players.LocalPlayer.Character:FindFirstChild("UwU Fly") then 
                      game.Players.LocalPlayer.Character:FindFirstChild("UwU Fly"):Destroy()
                  else
                      game.Players.LocalPlayer.Backpack:FindFirstChild("UwU Fly"):Destroy()
                     end
                  end
              end)

        ToolsPage.AddToggle("Glide", false, function(State)
            getgenv().Glide = State
            while getgenv().Glide == true do task.wait() 
                pcall(function()
                if not game.Players.LocalPlayer.Character:FindFirstChild("Glide") and not game.Players.LocalPlayer.Backpack:FindFirstChild("Glide") then
                    local outfit = Instance.new("Tool", game.Players.LocalPlayer.Backpack)
                    outfit.Name = "Glide"
                    outfit.RequiresHandle = false
                    local player = game.Players.LocalPlayer
                    while true do
                        wait()
                        if player.Character then
                            break
                        end
                    end
                    local Animation = Instance.new("Animation")
                    Animation.AnimationId = "rbxassetid://3084858603"
                    local char = player.Character
                    local Humanoid = char:WaitForChild("Humanoid")
                    local tool = outfit
                    local u1 = false
                    local u3 = false
                    local u4 = Humanoid:LoadAnimation(Animation)
                    tool.Equipped:connect(function(p1)
                        p1.Button1Down:connect(function()
                            if u1 or Humanoid.FloorMaterial ~= Enum.Material.Air then
                                return
                            end
                            u1 = true
                            u3 = true
                            u4:Play()
                            local v5 = Instance.new("BodyVelocity", char.HumanoidRootPart)
                            v5.MaxForce = Vector3.new(50000, 50000, 50000)
                            v5.Velocity = (char.HumanoidRootPart.CFrame * CFrame.Angles(math.rad(-18), 0, 0)).lookVector * 40
                            while u3 and Humanoid.FloorMaterial == Enum.Material.Air do
                                v5.Velocity = (char.HumanoidRootPart.CFrame * CFrame.Angles(math.rad(-18), 0, 0)).lookVector * 40
                                game:GetService("RunService").Heartbeat:wait()
                            end
                            v5:Destroy()
                            u3 = false
                            u4:Stop()
                            wait(0.3)
                            u1 = false
                        end)
                        p1.Button1Up:connect(function()
                            u3 = false
                        end)
                    end)
                    tool.Unequipped:connect(function()
                        u3 = false
                    end)
                end
            end)
            end
            if not getgenv().Glide then 
                if  game.Players.LocalPlayer.Character:FindFirstChild("Glide") then 
                    game.Players.LocalPlayer.Character:FindFirstChild("Glide"):Destroy()
                else
                    game.Players.LocalPlayer.Backpack:FindFirstChild("Glide"):Destroy()
                   end
                end
            end)


            ToolsPage.AddToggle("Bat Attack", nil, function(State)
                getgenv().batattack = State 
                
                while getgenv().batattack do task.wait()
                pcall(function()
                repeat task.wait()
                until game.Players.LocalPlayer.Character:FindFirstChild("FULLY_LOADED_CHAR")
                
                if not game.Players.LocalPlayer.Character:FindFirstChild("Bat Attack") and not game.Players.LocalPlayer.Backpack:FindFirstChild("Bat Attack") then
                local b 
                local batflingtool = Instance.new("Tool")
                batflingtool.RequiresHandle = false 
                batflingtool.Name = "Bat Attack"
                batflingtool.Parent = game.Players.LocalPlayer.Backpack
                batflingtool.CanBeDropped = false
                
                batflingtool.Activated:Connect(function()
                  game.Players:Chat("/e a")
                 game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("[Phone]").Parent = game:GetService("Players").LocalPlayer.Character
                        game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Humanoid"):UnequipTools()
                         game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Bat Attack").Parent = game:GetService("Players").LocalPlayer.Character
                         if game.Players.LocalPlayer.Backpack:FindFirstChild("[Bat]") then 
                        local bat = game.Players.LocalPlayer.Backpack:FindFirstChild("[Bat]")
                game:GetService("ReplicatedStorage").MainEvent:FireServer("RingTone",5696182212)
                        bat.Parent = game.Players.LocalPlayer.Character
                        task.wait()
                        local batswinganim = Instance.new("Animation")
                        batswinganim.AnimationId = "rbxassetid://3172948973"
                        local humanoid = game.Players.LocalPlayer.Character:WaitForChild("Humanoid")
                        local anim = humanoid:LoadAnimation(batswinganim)
                        anim:Play()
                        anim:AdjustSpeed(1.3)
                       
                        wait(1)
                        game.Players:Chat("/e cm")
                        task.wait()
                
                        if game.Players.LocalPlayer.Character:FindFirstChild("Bat Attack") then 
                         b = true 
                        end
                        task.wait(2)
                         game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("[Phone]").Parent = game:GetService("Players").LocalPlayer.Character
                        game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Humanoid"):UnequipTools()
                        task.wait()
                        if b == true then 
                            game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Bat Attack").Parent = game:GetService("Players").LocalPlayer.Character
                        b = false 
                          end 
                        end
                      end)
                     end
                  end)
                 end
                if not getgenv().bad then 
                    if  game.Players.LocalPlayer.Character:FindFirstChild("Bat Attack") then 
                        game.Players.LocalPlayer.Character:FindFirstChild("Bat Attack"):Destroy()
                    else
                        game.Players.LocalPlayer.Backpack:FindFirstChild("Bat Attack"):Destroy()
                   end
                  end
                end)


